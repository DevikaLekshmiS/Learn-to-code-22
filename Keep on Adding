int main() {
    int n,sum=0,count=0;
     do
    {
        scanf("%d",&n);
        count++;
      sum=sum+n;
   if(count>=2 && n!=0)
    {
     printf("%d\n",sum);
    }
    } 
    while(n!=0);
    
    
    
    
    
    
    

     

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
