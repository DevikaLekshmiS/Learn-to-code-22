#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int a,b,i,check=0;
    scanf("%d",&a);
    b = a / 2;

for(i=1;i<=b;i++)
{
    if( b % i == 0)
     {
        check++;
     }
}
if( check==2 )
    {
    printf("Number is prime");
    }
else
    {
    printf("Number is not prime");
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
