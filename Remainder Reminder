#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int a,b,c,d;
    scanf("%d\n",&a);
    scanf("%d\n",&b);
    scanf("%d\n",&c);
    scanf("%d\n",&d);
    
    printf("%d",a%b+c%d);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
