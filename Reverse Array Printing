#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int i,n;
    scanf("%d",&n);
    printf("Enter the number of elements in the array : \n");
    int numbers[n];
    for(i=0;i<=n;i++)
        {
        scanf("%d",&numbers[i]);
        }
    for(i=n-1;i>=0;i--)
        {
        printf("%d\n",numbers[i]);
        }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
