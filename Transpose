#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int i,j,N,M;
    int A[100][100],B[100][100];
    scanf("%d",&N);
    scanf("%d",&M);
    for(i=0;i<N;i++)
        {
        for(j=0;j<M;j++)
             {
              scanf("%d",&A[i][j]);
             }
        }
    for(i=0;i<N;i++) 
        {
        for(j=0;j<M;j++)
             {
           B[j][i] = A[i][j];
             }
        }
        for(i=0;i<N;i++) 
            {
            for(j=0;j<M;j++)
             {
             printf("%d ",B[i][j]);
             if(j==N-1)
                 printf("\n");
            }
            }
        


    
    


    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
